{
    "name": "prs1378/filament-tickets",
    "description": "Ticket system plugin for FilamentPHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "prs1378\\FilamentTickets\\": "src/"
        }
    },
    "require": {
        "php": "^8.2|^8.3",
        "filament/filament": "^3.0",
        "livewire/livewire": "^3.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "prs1378\\FilamentTickets\\FilamentTicketsServiceProvider"
            ]
        }
    }
    "minimum-stability": "stable",
    "prefer-stable": true
} 
